{
  "extends": "@salesforce/dev-config/tsconfig-strict",
  "compilerOptions": {
    "outDir": "lib",
    "include": ["./force-app/**/*.ts"],
    "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": ["es2019"],
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "alwaysStrict": true,
    "noUnusedLocals": true
    }
  }
}
